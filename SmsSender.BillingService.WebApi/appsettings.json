{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "BillingDbContext": "Password=#{pass};Host=#{host};Database=#{database}; Persist Security Info=True;Username=#{username}"
  },
  "RabbitConnectionOptions": {
    "Host": "#{host}",
    "Port": 5672,
    "User": "#{user}",
    "Password": "#{password}"
  },
  "RabbitQueueOptions": {
    "QueueOptions": {
      "email_notification": {
        "Name": "email_notification"
      }
    }
  },
  "RedisOptions": {
    "Connection": "localhost"
  },
  "AllowedHosts": "*"
}
